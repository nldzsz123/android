<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:splitMotionEvents="false"
    android:visibility="visible">

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="40px"
        android:layout_marginTop="32px"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="@string/intelligent_battery_info"
            android:textColor="@color/color_ffffff"
            android:textSize="40px" />


        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="472px"
                android:layout_height="wrap_content"
                android:layout_marginTop="24px"
                android:layout_marginRight="62px"
                android:orientation="vertical">


                <com.zhy.autolayout.AutoRelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/voltage"
                        android:textColor="@color/color_b8b8b8"
                        android:textSize="30px" />

                    <TextView
                        android:id="@+id/voltage_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="175px"
                        android:text="0"
                        android:textColor="@color/color_ffffff"
                        android:textSize="33px"
                        android:textStyle="bold" />
                </com.zhy.autolayout.AutoRelativeLayout>

                <com.zhy.autolayout.AutoRelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="33px"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/temperature"
                        android:textColor="@color/color_b8b8b8"
                        android:textSize="30px" />

                    <TextView
                        android:id="@+id/temperature_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="175px"
                        android:text="0"
                        android:textColor="@color/color_ffffff"
                        android:textSize="33px"
                        android:textStyle="bold" />
                </com.zhy.autolayout.AutoRelativeLayout>

                <com.zhy.autolayout.AutoRelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="33px"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/battery_capacity"
                        android:textColor="@color/color_b8b8b8"
                        android:textSize="30px" />

                    <TextView
                        android:id="@+id/battery_capacity_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="175px"
                        android:text="0"
                        android:textColor="@color/color_ffffff"
                        android:textSize="33px"
                        android:textStyle="bold" />
                </com.zhy.autolayout.AutoRelativeLayout>

                <com.zhy.autolayout.AutoRelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="33px"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/electricity"
                        android:textColor="@color/color_b8b8b8"
                        android:textSize="30px" />

                    <TextView
                        android:id="@+id/electricity_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="175px"
                        android:text="0"
                        android:textColor="@color/color_ffffff"
                        android:textSize="33px"
                        android:textStyle="bold" />
                </com.zhy.autolayout.AutoRelativeLayout>

                <com.zhy.autolayout.AutoRelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="33px"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/cycles"
                        android:textColor="@color/color_b8b8b8"
                        android:textSize="30px" />

                    <TextView
                        android:id="@+id/cycles_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="175px"
                        android:text="0"
                        android:textColor="@color/color_ffffff"
                        android:textSize="33px"
                        android:textStyle="bold" />
                </com.zhy.autolayout.AutoRelativeLayout>


            </com.zhy.autolayout.AutoLinearLayout>

            <ImageView
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="@color/color_ffffff" />

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <com.zhy.autolayout.AutoRelativeLayout
                        android:id="@+id/first_voltage_ly"
                        android:layout_width="144px"
                        android:layout_height="240px"
                        android:background="@drawable/borderffffff_bg"
                        android:padding="8px">

                        <ImageView
                            android:id="@+id/first_voltage_img"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:background="@drawable/fill00cf00_bg"></ImageView>
                    </com.zhy.autolayout.AutoRelativeLayout>

                    <TextView
                        android:id="@+id/first_voltage_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="N/A"
                        android:textColor="@color/color_ffffff"
                        android:textSize="30px" />
                </com.zhy.autolayout.AutoLinearLayout>

                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <com.zhy.autolayout.AutoRelativeLayout
                        android:id="@+id/second_voltage_ly"
                        android:layout_width="144px"
                        android:layout_height="240px"
                        android:background="@drawable/borderffffff_bg"
                        android:padding="8px">

                        <ImageView
                            android:id="@+id/second_voltage_img"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:background="@drawable/fillfe9700_bg"></ImageView>
                    </com.zhy.autolayout.AutoRelativeLayout>

                    <TextView
                        android:id="@+id/second_voltage_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="N/A"
                        android:textColor="@color/color_ffffff"
                        android:textSize="30px" />
                </com.zhy.autolayout.AutoLinearLayout>

                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <com.zhy.autolayout.AutoRelativeLayout
                        android:id="@+id/third_voltage_ly"
                        android:layout_width="144px"
                        android:layout_height="240px"
                        android:background="@drawable/borderffffff_bg"
                        android:padding="8px">

                        <ImageView
                            android:id="@+id/third_voltage_img"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:background="@drawable/fill00cf00_bg"></ImageView>
                    </com.zhy.autolayout.AutoRelativeLayout>

                    <TextView
                        android:id="@+id/third_voltage_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="N/A"
                        android:textColor="@color/color_ffffff"
                        android:textSize="30px" />
                </com.zhy.autolayout.AutoLinearLayout>

                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <com.zhy.autolayout.AutoRelativeLayout
                        android:id="@+id/fourth_voltage_ly"
                        android:layout_width="144px"
                        android:layout_height="240px"
                        android:background="@drawable/borderffffff_bg"
                        android:padding="8px">

                        <ImageView
                            android:id="@+id/fourth_voltage_img"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:background="@drawable/fillf34235_bg"></ImageView>
                    </com.zhy.autolayout.AutoRelativeLayout>

                    <TextView
                        android:id="@+id/fourth_voltage_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="N/A"
                        android:textColor="@color/color_ffffff"
                        android:textSize="30px" />
                </com.zhy.autolayout.AutoLinearLayout>


            </com.zhy.autolayout.AutoLinearLayout>
        </com.zhy.autolayout.AutoLinearLayout>
    </com.zhy.autolayout.AutoLinearLayout>


</com.zhy.autolayout.AutoLinearLayout>



