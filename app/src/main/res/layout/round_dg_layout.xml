<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_view_pager"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_transparent"
    tools:context=".ui.activity.ActivationActivity">

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="672px"
        android:layout_height="417px"
        android:background="@drawable/roundb8b8b890_bg"
        android:orientation="vertical">

        <TextView
            android:id="@+id/around_dg_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="17px"
            android:text="@string/surrounded_by_arget"
            android:textColor="@color/color_ffffff"
            android:textSize="30px"
            android:textStyle="bold" />

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="150px"
            android:layout_marginTop="16px"
            android:background="@color/color_333333_50"
            android:gravity="center">

            <TextView
                android:id="@+id/around_start_center_tv"
                android:layout_width="wrap_content"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:text="@string/move_plane_over_canter_circle"
                android:textColor="@color/color_ffffff"
                android:textSize="30px"
                android:visibility="gone" />

            <com.zhy.autolayout.AutoLinearLayout
                android:id="@+id/around_speed_ly"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:orientation="horizontal">

                    <com.zhy.autolayout.AutoLinearLayout
                        android:id="@+id/around_height_ly"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/around_height"
                            android:textColor="@color/color_ffffff"
                            android:textSize="30px" />

                        <TextView
                            android:id="@+id/around_height_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="10"
                            android:textColor="@color/color_f34235"
                            android:textSize="50px"
                            android:textStyle="bold"
                            android:visibility="visible" />

                        <TextView
                            android:id="@+id/taking_panor_photo_dg_tv_end"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/m"
                            android:textColor="@color/color_ffffff"
                            android:textSize="30px" />
                    </com.zhy.autolayout.AutoLinearLayout>

                    <com.zhy.autolayout.AutoLinearLayout
                        android:id="@+id/around_radius_ly"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="48px"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/around_radius"
                            android:textColor="@color/color_ffffff"
                            android:textSize="30px" />

                        <TextView
                            android:id="@+id/around_radius_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="10"
                            android:textColor="@color/color_f34235"
                            android:textSize="50px"
                            android:textStyle="bold"
                            android:visibility="visible" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/m"
                            android:textColor="@color/color_ffffff"
                            android:textSize="30px" />
                    </com.zhy.autolayout.AutoLinearLayout>
                </com.zhy.autolayout.AutoLinearLayout>

                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15px"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <com.zhy.autolayout.AutoLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="50px"
                            android:layout_height="50px"
                            android:src="@mipmap/counterclockwise_img" />
                    </com.zhy.autolayout.AutoLinearLayout>


                    <com.feipai.flypai.ui.view.CenterPointSeekBar
                        android:id="@+id/around_speed_seekbar"
                        android:layout_width="470px"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="25px"
                        android:layout_marginRight="25px"
                        android:progressDrawable="@android:color/transparent"
                        android:theme="@style/SeekBarThem"
                        app:backgroundDrawable="@drawable/seekbar_editor_progress_border"
                        app:defaultBackgroundColor="@color/color_transparent"
                        app:defaultBackgroundRangeColor="@color/color_transparent"
                        app:linHeight="16"
                        app:maxValue="1000"
                        app:minValue="-1000"
                        app:progressDrawable="@drawable/seekbar_editor_progress_gradient" />

                    <com.zhy.autolayout.AutoLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="50px"
                            android:layout_height="50px"
                            android:src="@mipmap/clockwise" />
                    </com.zhy.autolayout.AutoLinearLayout>


                </com.zhy.autolayout.AutoLinearLayout>
            </com.zhy.autolayout.AutoLinearLayout>


        </com.zhy.autolayout.AutoLinearLayout>

        <TextView
            android:id="@+id/around_dg_start_hint_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="8px"
            android:text="@string/around_make_sure_sufficient_power"
            android:textColor="@color/color_ffffff_50"
            android:textSize="28px"
            android:visibility="visible" />


        <com.zhy.autolayout.AutoLinearLayout
            android:id="@+id/around_speed_value_ly"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8px"
            android:gravity="center">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/current_around_speed"
                android:textColor="@color/color_ffffff"
                android:textSize="30px" />

            <TextView
                android:id="@+id/around_speed_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0.3"
                android:textColor="@color/color_00cf00"
                android:textSize="40px"
                android:textStyle="bold"
                android:visibility="visible" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="m/s"
                android:textColor="@color/color_ffffff"
                android:textSize="30px" />
        </com.zhy.autolayout.AutoLinearLayout>

        <TextView
            android:id="@+id/around_comfirm_bnt"
            android:layout_width="344px"
            android:layout_height="88px"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="9px"
            android:layout_marginBottom="24px"
            android:background="@drawable/login_button_selcetor"
            android:gravity="center"
            android:text="@string/setting"
            android:textColor="@color/color_ffffff"
            android:textStyle="bold" />

    </com.zhy.autolayout.AutoLinearLayout>
</com.zhy.autolayout.AutoRelativeLayout>
